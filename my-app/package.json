{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "electron.js",
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "ajv": "^8.12.0",
    "ajv-keywords": "^5.1.0",
    "bootstrap": "^5.3.7",
    "chart.js": "^4.5.0",
    "gpt4all": "^4.0.0",
    "node-llama-cpp": "^3.12.1",
    "papaparse": "^5.5.3",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.10",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "react-scripts": "5.0.1",
    "typescript": "^5.9.2",
    "web-vitals": "^2.1.4",
    "wordcloud": "^1.2.3",
    "xlsx": "^0.18.5"
  },
  "build": {
  "appId": "com.myapp.id",
  "productName": "VA Explorer",
  "files": [
    "electron.js",
    "package.json",
    "build/**/*"
  ],
  "directories": {
    "buildResources": "assets"
  },
  "win": {
    "target": "nsis"
  }
},
  "scripts": {
    "start": "concurrently --kill-others \"npm run react-start\" \"npm run electron-start\"",
    "react-start": "react-scripts start",
    "electron-start": "wait-on http://localhost:3000 && electron .",
    "build": "react-scripts build",
    "dist": "npm run build && electron-builder",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "npm_config_electron_prebuilt=true electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "electron": "^37.2.4",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "wait-on": "^8.0.4"
  }
}
